SELECT 
{
	 {
	([Time].[Fiscal Hierarchy Time Calculations 1].[Year to Date],[Measures].[Contribution]),
	([Time].[Fiscal Hierarchy Time Calculations 1].[Year to Date],[Measures].[Percentage of goal])
	 },
	 {
	 ([Time].[Fiscal Hierarchy Time Calculations 1].[Quarter to Date],[Measures].[Contribution]),
	 ([Time].[Fiscal Hierarchy Time Calculations 1].[Quarter to Date],[Measures].[Percentage of goal])
	 },
	 {
	 ([Time].[Fiscal Hierarchy Time Calculations 1].[Month to Date],[Measures].[Contribution]),
	 ([Time].[Fiscal Hierarchy Time Calculations 1].[Month to Date],[Measures].[Percentage of goal])
	 }	 
 }  
 ON COLUMNS, 
 {  [Accounts].[Account Department Name].Members
 } ON ROWS
FROM [GL_2008]
WHERE [Time].[Fiscal Month].&[2008-12-01T00:00:00]
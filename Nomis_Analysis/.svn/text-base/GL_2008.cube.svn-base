  <Kpis>
    <Kpi dwd:design-time-name="2bf84d62-9865-4974-81c8-10bd2e69c54c">
      <ID>KPI</ID>
      <Name>Profit Margin</Name>
      <DisplayFolder>Margins</DisplayFolder>
      <AssociatedMeasureGroupID>Fact GL</AssociatedMeasureGroupID>
      <Value>[Measures].[Gross Margin %]</Value>
      <Goal>[Measures].[Gross Margin % Budget]</Goal>
      <Status>CASE
   WHEN KpiValue( "Profit Margin" ) &gt;=
      KpiGoal ( "Profit Margin" ) 
   THEN 1
   WHEN 
      KpiValue( "Profit Margin" ) &gt;= 
         .90 * KpiGoal ( "Profit Margin" )
      AND
         KpiValue( "Profit Margin" ) &lt; 
            KpiGoal ( "Profit Margin" ) 
    THEN 0
    ELSE -1
END
</Status>
      <Trend>CASE
    WHEN 
       [Time].[Fiscal].CURRENTMEMBER.LEVEL Is [Time].[Fiscal].[(All)]
    THEN 0
    WHEN 
       VBA!ABS(
           KpiValue( "Profit Margin" ) - 
             (KpiValue ("Profit Margin" ),
                 PARALLELPERIOD([Time].[Fiscal].[Fiscal Year], 1,
                    [Time].[Fiscal].CURRENTMEMBER)) /
                 ( KpiValue ( "Profit Margin" ),
             PARALLELPERIOD([Time].[Fiscal].[Fiscal Year], 1,
                  [Time].[Fiscal].CURRENTMEMBER))  
        ) &lt;=.02
    THEN 0
    WHEN 
       KpiValue( "Profit Margin" ) - 
          (KpiValue ( "Profit Margin" ),
              PARALLELPERIOD([Time].[Fiscal].[Fiscal Year], 1,
                 [Time].[Fiscal].CURRENTMEMBER)) /
                    (KpiValue ( "Profit Margin" ),
                 PARALLELPERIOD([Time].[Fiscal].[Fiscal Year], 1,
              [Time].[Fiscal].CURRENTMEMBER)
           )  &gt;.02
    THEN 1
    ELSE -1
END
</Trend>
      <TrendGraphic>Standard Arrow</TrendGraphic>
      <StatusGraphic>Road Signs</StatusGraphic>
    </Kpi>

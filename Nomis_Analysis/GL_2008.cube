  <MdxScripts>
    <MdxScript dwd:design-time-name="3acaa311-80c8-4237-9945-82fe16c35836">
      <ID>MdxScript</ID>
      <Name>MdxScript</Name>
      <CreatedTimestamp>0001-01-01T05:00:00Z</CreatedTimestamp>
      <LastSchemaUpdate>0001-01-01T05:00:00Z</LastSchemaUpdate>
      <Commands>
        <Command>
          <Text>/*
The CALCULATE command controls the aggregation of leaf cells in the cube.
If the CALCULATE command is deleted or modified, the data within the cube is affected.
You should edit this command only if you manually specify how the cube is aggregated.
*/
CALCULATE;            
CREATE SET CURRENTCUBE.[Services]
 AS {[Accounts].[Account Department Name].&amp;[Reliability Products],
 [Accounts].[Account Department Name].&amp;[Reliability Services],
 [Accounts].[Account Department Name].&amp;[Field Instrumentation],
 [Accounts].[Account Department Name].&amp;[Alcan],
 [Accounts].[Account Department Name].&amp;[Engineering],
 [Accounts].[Account Department Name].&amp;[Automation],
 [Accounts].[Account Department Name].&amp;[Pratt &amp; Whitney]
} ;       
CREATE SET CURRENTCUBE.[No Revenue Departments]
 AS {[Accounts].[Account Department Name].&amp;[Warehouse],
 [Accounts].[Account Department Name].&amp;[Sales Operation Atlantic],
 [Accounts].[Account Department Name].&amp;[Sales Operation Montreal],
 [Accounts].[Account Department Name].&amp;[Service Support Team],
 [Accounts].[Account Department Name].&amp;[Finance]} ;                   
CREATE SET CURRENTCUBE.[Expenses]
 AS {[Accounts].[Account Category Id].&amp;[4],[Accounts].[Account Category Id].&amp;[5],[Accounts].[Account Category Id].&amp;[6]};                  
CREATE SET CURRENTCUBE.[Sales]
 AS [Accounts].[Account Category Id].&amp;[1];                  
CREATE SET CURRENTCUBE.[COGS]
 AS [Accounts].[Account Category Id].&amp;[2];             
CREATE SET CURRENTCUBE.[Q&amp;O]
 AS {[Accounts].[Account Department Name].&amp;[Sales Montreal],[Accounts].[Account Department Name].&amp;[Sales Operation Montreal],[Accounts].[Account Department Name].&amp;[Sales Operation Atlantic]} ;        
CREATE MEMBER CURRENTCUBE.[MEASURES].[Department Profit]
 AS iif([Sales Actual] &lt;&gt; 0,([Sales Actual]+[COGS Actual]), null), 
FORMAT_STRING = "Currency", 
VISIBLE = 1;        
CREATE MEMBER CURRENTCUBE.[MEASURES].[Department Profit Budget]
 AS iif([Sales Budget] &lt;&gt; 0,([Sales Budget]+[COGS Budget]), null), 
FORMAT_STRING = "Currency", 
VISIBLE = 1;          
CREATE MEMBER CURRENTCUBE.[MEASURES].[Department Margin]
 AS iif([Sales Actual] &lt;&gt; 0,([Sales Actual]+[COGS Actual])/[Sales Actual], null), 
FORMAT_STRING = "Percent", 
VISIBLE = 1;          
CREATE MEMBER CURRENTCUBE.[MEASURES].[Department Margin Budget]
 AS iif([Sales Budget] &lt;&gt; 0,([Sales Budget]+[COGS Budget])/[Sales Budget], null), 
FORMAT_STRING = "Percent", 
VISIBLE = 1;              
CREATE MEMBER CURRENTCUBE.[MEASURES].[Percentage of goal]
 AS case [Accounts].[Account Department Name].currentmember.name 
when "Warehouse"
then (([Measures].[Total]-[Measures].[Amount])/[Measures].[Amount])+1
when "Sales Operation" 
then (([Measures].[Total]-[Measures].[Amount])/[Measures].[Amount])+1
when "Finance"
then (([Measures].[Total]-[Measures].[Amount])/[Measures].[Amount])+1
else [Measures].[Amount]/[Measures].[Total] end, 
VISIBLE = 1;                  
CREATE MEMBER CURRENTCUBE.[MEASURES].[Gross Margin]
 AS Aggregate([Sales], [Measures].[Amount]) + Aggregate([COGS], [Measures].[Amount]), 
FORMAT_STRING = "Currency", 
VISIBLE = 1;                
CREATE MEMBER CURRENTCUBE.[MEASURES].[Gross Margin Budget]
 AS Aggregate([Sales], [Measures].[Total]) + Aggregate([COGS], [Measures].[Total]), 
FORMAT_STRING = "Currency", 
VISIBLE = 1;                  
CREATE MEMBER CURRENTCUBE.[MEASURES].[Gross Margin %]
 AS iif(Aggregate([Sales], [Measures].[Amount]) = 0, 0, (Aggregate([Sales], [Measures].[Amount]) + Aggregate([COGS], [Measures].[Amount])) / Aggregate([Sales], [Measures].[Amount])), 
FORMAT_STRING = "Percent", 
VISIBLE = 1;                
CREATE MEMBER CURRENTCUBE.[MEASURES].[Gross Margin % Budget]
 AS iif(Aggregate([Sales], [Measures].[Total]) = 0, 0, (Aggregate([Sales], [Measures].[Total]) + Aggregate([COGS], [Measures].[Total])) / Aggregate([Sales], [Measures].[Total])), 
FORMAT_STRING = "Percent", 
VISIBLE = 1;                 
CREATE MEMBER CURRENTCUBE.[MEASURES].Contribution
 AS [Measures].[Amount], 
FORMAT_STRING = "Currency", 
VISIBLE = 1;                 
CREATE MEMBER CURRENTCUBE.[MEASURES].[Sales Actual]
 AS Aggregate([Sales], [Measures].[Amount]), 
FORMAT_STRING = "Currency", 
VISIBLE = 1;                
CREATE MEMBER CURRENTCUBE.[MEASURES].[Sales Budget]
 AS Aggregate([Sales], [Measures].[Total]), 
FORMAT_STRING = "Currency", 
VISIBLE = 1;                 
CREATE MEMBER CURRENTCUBE.[MEASURES].[Expenses Actual]
 AS Aggregate([Expenses], [Measures].[Amount]), 
FORMAT_STRING = "Currency", 
VISIBLE = 1;                
CREATE MEMBER CURRENTCUBE.[MEASURES].[Expenses Budget]
 AS Aggregate([Expenses], [Measures].[Total]), 
FORMAT_STRING = "Currency", 
VISIBLE = 1;                 
CREATE MEMBER CURRENTCUBE.[MEASURES].[COGS Actual]
 AS Aggregate([COGS], [Measures].[Amount]), 
FORMAT_STRING = "Currency", 
VISIBLE = 1;                
CREATE MEMBER CURRENTCUBE.[MEASURES].[COGS Budget]
 AS Aggregate([COGS], [Measures].[Total]), 
FORMAT_STRING = "Currency", 
VISIBLE = 1;                
CREATE MEMBER CURRENTCUBE.[MEASURES].[Var %]
 AS iif([Measures].[Amount] = 0, null,([Measures].[Amount]-[Measures].[Total])/[Measures].[Amount]), 
FORMAT_STRING = "Percent", 
VISIBLE = 1;                
CREATE MEMBER CURRENTCUBE.[MEASURES].[Var $]
 AS iif([Measures].[Amount] = 0,null,[Measures].[Amount]-[Measures].[Total]), 
FORMAT_STRING = "Currency", 
VISIBLE = 1;               


/*
  Begin Time Intelligence script for the [Time].[Fiscal Hierarchy] hierarchy.
*/

Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Year to Date] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Quarter to Date] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Month to Date] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Twelve Months to Date] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Twelve Month Moving Average] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Six Month Moving Average] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Three Month Moving Average] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Year Over Year Growth %] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Year Over Year Growth] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Quarter Over Quarter Growth %] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Quarter Over Quarter Growth] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Month Over Month Growth %] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Month Over Month Growth] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Day Over Day Growth %] 
  As "NA";   
  
Create Member 
  CurrentCube.[Time].[Fiscal Hierarchy Time Calculations 1].[Day Over Day Growth] 
  As "NA";   
  


// Year to Date
  (
    [Time].[Fiscal Hierarchy Time Calculations 1].[Year to Date],
    [Time].[Fiscal Year].[Fiscal Year].Members,
	[Time].[Date].Members,
    {
      [Measures].[Department Profit],
      [Measures].[Department Profit Budget],
      [Measures].[Department Margin],
      [Measures].[Department Margin Budget],
      [Measures].[Percentage of goal],
      [Measures].[Gross Margin],
      [Measures].[Gross Margin Budget],
      [Measures].[Gross Margin %],
      [Measures].[Gross Margin % Budget],
      [Measures].[Contribution],
      [Measures].[Sales Actual],
      [Measures].[Sales Budget],
      [Measures].[Expenses Actual],
      [Measures].[Expenses Budget],
      [Measures].[COGS Actual],
      [Measures].[COGS Budget],
      [Measures].[Var %],
      [Measures].[Var $],
      [Measures].[Total],
      [Measures].[Amount]
    }
   
  ) =

  Aggregate(
             { [Time].[Fiscal Hierarchy Time Calculations 1].DefaultMember } *
             PeriodsToDate(
                            [Time].[Fiscal Hierarchy].[Fiscal Year],
                            [Time].[Fiscal Hierarchy].CurrentMember
             )
  );   

  
// Quarter to Date
  ( 
	[Time].[Fiscal Hierarchy Time Calculations 1].[Quarter to Date],
    [Time].[Fiscal Quarter].[Fiscal Quarter].Members,
	[Time].[Date].Members,
    {
      [Measures].[Department Profit],
      [Measures].[Department Profit Budget],
      [Measures].[Department Margin],
      [Measures].[Department Margin Budget],
      [Measures].[Percentage of goal],
      [Measures].[Gross Margin],
      [Measures].[Gross Margin Budget],
      [Measures].[Gross Margin %],
      [Measures].[Gross Margin % Budget],
      [Measures].[Contribution],
      [Measures].[Sales Actual],
      [Measures].[Sales Budget],
      [Measures].[Expenses Actual],
      [Measures].[Expenses Budget],
      [Measures].[COGS Actual],
      [Measures].[COGS Budget],
      [Measures].[Var %],
      [Measures].[Var $],
      [Measures].[Total],
      [Measures].[Amount]
    }
   
  ) =

  Aggregate(
             { [Time].[Fiscal Hierarchy Time Calculations 1].DefaultMember } *
             PeriodsToDate(
                            [Time].[Fiscal Hierarchy].[Fiscal Quarter],
                            [Time].[Fiscal Hierarchy].CurrentMember
             )
  );   

  
// Month to Date
  ( 
	[Time].[Fiscal Hierarchy Time Calculations 1].[Month to Date],
    [Time].[Fiscal Month].[Fiscal Month].Members,
	[Time].[Date].Members,
    {
      [Measures].[Department Profit],
      [Measures].[Department Profit Budget],
      [Measures].[Department Margin],
      [Measures].[Department Margin Budget],
      [Measures].[Percentage of goal],
      [Measures].[Gross Margin],
      [Measures].[Gross Margin Budget],
      [Measures].[Gross Margin %],
      [Measures].[Gross Margin % Budget],
      [Measures].[Contribution],
      [Measures].[Sales Actual],
      [Measures].[Sales Budget],
      [Measures].[Expenses Actual],
      [Measures].[Expenses Budget],
      [Measures].[COGS Actual],
      [Measures].[COGS Budget],
      [Measures].[Var %],
      [Measures].[Var $],
      [Measures].[Total],
      [Measures].[Amount]
    }
   
  ) =

  Aggregate(
             { [Time].[Fiscal Hierarchy Time Calculations 1].DefaultMember } *
             PeriodsToDate(
                            [Time].[Fiscal Hierarchy].[Fiscal Month],
                            [Time].[Fiscal Hierarchy].CurrentMember
             )
  );   

  
// Twelve Months to Date
  ( 
	[Time].[Fiscal Hierarchy Time Calculations 1].[Twelve Months to Date],
    [Time].[Fiscal Month].[Fiscal Month].Members,
	[Time].[Date].Members,
    {
      [Measures].[Department Profit],
      [Measures].[Department Profit Budget],
      [Measures].[Department Margin],
      [Measures].[Department Margin Budget],
      [Measures].[Percentage of goal],
      [Measures].[Gross Margin],
      [Measures].[Gross Margin Budget],
      [Measures].[Gross Margin %],
      [Measures].[Gross Margin % Budget],
      [Measures].[Contribution],
      [Measures].[Sales Actual],
      [Measures].[Sales Budget],
      [Measures].[Expenses Actual],
      [Measures].[Expenses Budget],
      [Measures].[COGS Actual],
      [Measures].[COGS Budget],
      [Measures].[Var %],
      [Measures].[Var $],
      [Measures].[Total],
      [Measures].[Amount]
    }
   
  ) =
    
  Aggregate(
             { [Time].[Fiscal Hierarchy Time Calculations 1].DefaultMember } *
             {
               ParallelPeriod(
                               [Time].[Fiscal Hierarchy].[Fiscal Month],
                               11,
                               [Time].[Fiscal Hierarchy].CurrentMember
               ) : [Time].[Fiscal Hierarchy].CurrentMember
             }
  );   

  
// Twelve Month Moving Average  
  ( 
	[Time].[Fiscal Hierarchy Time Calculations 1].[Twelve Month Moving Average],
    [Time].[Fiscal Month].[Fiscal Month].Members,
	[Time].[Date].Members,
    {
      [Measures].[Department Profit],
      [Measures].[Department Profit Budget],
      [Measures].[Department Margin],
      [Measures].[Department Margin Budget],
      [Measures].[Percentage of goal],
      [Measures].[Gross Margin],
      [Measures].[Gross Margin Budget],
      [Measures].[Gross Margin %],
      [Measures].[Gross Margin % Budget],
      [Measures].[Contribution],
      [Measures].[Sales Actual],
      [Measures].[Sales Budget],
      [Measures].[Expenses Actual],
      [Measures].[Expenses Budget],
      [Measures].[COGS Actual],
      [Measures].[COGS Budget],
      [Measures].[Var %],
      [Measures].[Var $],
      [Measures].[Total],
      [Measures].[Amount]
    }
   
  ) =
    
  Avg(
       {
         ParallelPeriod(
                         [Time].[Fiscal Hierarchy].[Fiscal Month],
                         11,
                         [Time].[Fiscal Hierarchy].CurrentMember
         ) : [Time].[Fiscal Hierarchy].CurrentMember
       },
      [Time].[Fiscal Hierarchy Time Calculations 1].DefaultMember
  );   

  
// Six Month Moving Average  
  ( 
	[Time].[Fiscal Hierarchy Time Calculations 1].[Six Month Moving Average],
    [Time].[Fiscal Month].[Fiscal Month].Members,
	[Time].[Date].Members,
    {
      [Measures].[Department Profit],
      [Measures].[Department Profit Budget],
      [Measures].[Department Margin],
      [Measures].[Department Margin Budget],
      [Measures].[Percentage of goal],
      [Measures].[Gross Margin],
      [Measures].[Gross Margin Budget],
      [Measures].[Gross Margin %],
      [Measures].[Gross Margin % Budget],
      [Measures].[Contribution],
      [Measures].[Sales Actual],
      [Measures].[Sales Budget],
      [Measures].[Expenses Actual],
      [Measures].[Expenses Budget],
      [Measures].[COGS Actual],
      [Measures].[COGS Budget],
      [Measures].[Var %],
      [Measures].[Var $],
      [Measures].[Total],
      [Measures].[Amount]
    }
   
  ) =
    
  Avg(
       {
         ParallelPeriod(
                         [Time].[Fiscal Hierarchy].[Fiscal Month],
                         5,
                         [Time].[Fiscal Hierarchy].CurrentMember
         ) : [Time].[Fiscal Hierarchy].CurrentMember
       },
      [Time].[Fiscal Hierarchy Time Calculations 1].DefaultMember
  );   

  
// Three Month Moving Average  
  ( 
	[Time].[Fiscal Hierarchy Time Calculations 1].[Three Month Moving Average],
    [Time].[Fiscal Month].[Fiscal Month].Members,
	[Time].[Date].Members,
    {
      [Measures].[Department Profit],
      [Measures].[Department Profit Budget],
      [Measures].[Department Margin],
      [Measures].[Department Margin Budget],
      [Measures].[Percentage of goal],
      [Measures].[Gross Margin],
      [Measures].[Gross Margin Budget],
      [Measures].[Gross Margin %],
      [Measures].[Gross Margin % Budget],
      [Measures].[Contribution],
      [Measures].[Sales Actual],
      [Measures].[Sales Budget],
      [Measures].[Expenses Actual],
      [Measures].[Expenses Budget],
      [Measures].[COGS Actual],
      [Measures].[COGS Budget],
      [Measures].[Var %],
      [Measures].[Var $],
      [Measures].[Total],
      [Measures].[Amount]
    }
   
  ) =
    
  Avg(
       {
         ParallelPeriod(
                         [Time].[Fiscal Hierarchy].[Fiscal Month],
                         2,
                         [Time].[Fiscal Hierarchy].CurrentMember
         ) : [Time].[Fiscal Hierarchy].CurrentMember
       },
      [Time].[Fiscal Hierarchy Time Calculations 1].DefaultMember
  );   

  
// Year Over Year Growth %  
  ( 
	[Time].[Fiscal Hierarchy Time Calculations 1].[Year Over Year Growth %],
    [Time].[Fiscal Year].[Fiscal Year].Members ( 1 ) : Null,
	[Time].[Date].Members,
    {
      [Measures].[Department Profit],
      [Measures].[Department Profit Budget],
      [Measures].[Department Margin],
      [Measures].[Department Margin Budget],
      [Measures].[Percentage of goal],
      [Measures].[Gross Margin],
      [Measures].[Gross Margin Budget],

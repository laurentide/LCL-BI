    <Kpi dwd:design-time-name="16208b79-b398-4c4f-a98b-b8de0f6a6703">
      <ID>KPI 3</ID>
      <Name>Sales</Name>
      <DisplayFolder>Sales</DisplayFolder>
      <AssociatedMeasureGroupID>Fact GL</AssociatedMeasureGroupID>
      <Value>[Measures].[Sales Actual]</Value>
      <Goal>[Measures].[Sales Budget]</Goal>
      <Status>CASE
   WHEN KpiValue( "Sales" ) &gt;=
      KpiGoal ( "Sales" ) 
   THEN 1
   WHEN 
      KpiValue( "Sales" ) &gt;= 
         .90 * KpiGoal ( "Sales" )
      AND
         KpiValue( "Sales" ) &lt; 
            KpiGoal ( "Sales" ) 
    THEN 0
    ELSE -1
END</Status>
      <Trend>CASE
    WHEN (KpiValue( "Sales" ),[Time].[Fiscal Month].CURRENTMEMBER) &gt; (KpiValue ("Sales" ),[Time].[Fiscal Month].CURRENTMEMBER.PREVMEMBER) 
    THEN 1
    WHEN (KpiValue( "Sales" ),[Time].[Fiscal Month].CURRENTMEMBER) &lt; (KpiValue ("Sales" ),[Time].[Fiscal Month].CURRENTMEMBER.PREVMEMBER) 
    THEN -1
    ELSE 0
END
</Trend>
      <TrendGraphic>Standard Arrow</TrendGraphic>
      <StatusGraphic>Road Signs</StatusGraphic>
    </Kpi>

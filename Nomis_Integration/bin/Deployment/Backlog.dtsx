<properties>
<property id="17" name="CommandTimeout" dataType="System.Int32" state="default" isArray="false" description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out." typeConverter="" UITypeEditor="" containsID="false" expressionType="None">0</property>
<property id="18" name="OpenRowset" dataType="System.String" state="default" isArray="false" description="Specifies the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="19" name="OpenRowsetVariable" dataType="System.String" state="default" isArray="false" description="Specifies the variable that contains the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="20" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=9.0.242.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">SELECT [KBQ9CD]                                                                    AS [SALES ORDER]

     --, KFQXST                                                                      AS [BUILDUP FLAG]

     --, KBAWDT                                                                      AS [ORDER DATE]

     , KFTRTX                                                                      AS [ORDER LINE ITEM #]

     --, MCD8NB                                                                      AS [COMPONENT SEQUENCE #]

     , [KBTJTX]                                                                    AS [CUSTOMER PO #]

     , [KBZXNB]                                                                    AS [CUSTOMER #]

     , [CACSTX]                                                                    AS [CUSTOMER NAME]

     , [GHUKCE]                                                                    AS [CURRENCY]

     , [KBRMCD]                                                                    AS ORIGINATOR

     , Name                                                                        AS CSR

     , case when [KFAYDT] = 0 then kbbmnb else kfaydt end                          AS FRSD

--     , KBBMNB                                                                                                         AS [FRSD HEADER]

     , [KFRDCD]                                                                    AS SALESMAN

     , CASE WHEN KFQXST IS NULL OR     KFQXST &lt;&gt; 'Y' THEN [KFC5AA] ELSE MCC5AA   END AS [PRODUCT CODE]

     , CASE WHEN KFQXST IS NULL OR     KFQXST &lt;&gt; 'Y' THEN [KFM8TX] ELSE [MCXHTX] END AS DESCRIPTION

     , CASE WHEN KFQXST IS NULL OR     KFQXST &lt;&gt; 'Y' THEN [KFXWNB] ELSE [MCE4NB] END AS [QUANTITY OPEN]

     , CASE WHEN KFQXST IS NULL OR     KFQXST &lt;&gt; 'Y' THEN [KFXWNB]*[KFLJVC]*[GCC9PC] ELSE [MCE4NB]*MCPHVA*GCC9PC END *

       CASE WHEN [KFJRCD]='99' OR MCX4ST = 'I' THEN -1 ELSE 1 END AS OPENVALUE

     , CASE WHEN KFQXST IS NULL OR     KFQXST &lt;&gt; 'Y' THEN [KFXWNB]*[KFI8VA]*GCC9PC ELSE [MCE4NB]*MCPIVA*GCC9PC END *

       CASE WHEN [KFJRCD]='99' OR MCX4ST = 'I' THEN -1 ELSE 1 END AS COSTVALUE

    -- , KFTUTX                                                                        AS [ORDER LINE SEQUENCE #]

   --  , KFC5AA                                                                        AS [ORDER LINE PRODUCT CODE]

   --  , KFT4ND                                                                        AS [ORDER LINE QTY ORDERED]

   --  , KFHPVA                                                                        AS [ORDER LINE LIST PRICE]

   --  , KFLJVC                                                                        AS [ORDER LINE UNIT NET]

   --  , KFI8VA                                                                        AS [ORDER LINE COST AMOUNT]

   --  , MCC5AA                                                                        AS [COMPONENT PRODUCT CODE]

   --  , MCE4NB                                                                        AS [COMPONENT QTY ORDERED]

    -- , MCPHVA                                                                        AS [COMPONENT LIST PRICE]

    -- , MCPIVA                                                                        AS [COMPONENT COST AMOUNT]

    -- , GCUKCE                                                                        AS [EXCHANGE CUR]

   --  , GCC9PC                                                                        AS [EXCHANGE RATE]

   ,PNOTE

FROM   NOMIS.S1018252.NOMDBF95.COHPHY01

       INNER JOIN NOMIS.S1018252.NOMDBF95.CUSPHY01

       ON     KBZXNB = CAZXNB

       INNER JOIN NOMIS.S1018252.NOMDBF95.CODPHY01

       ON     KBQ9CD = KFQ9CD

       LEFT JOIN NOMIS.S1018252.NOMDBF95.CDCPHY01

       ON     MCQ9CD = KBQ9CD

       AND    MCTUTX = KFTUTX

       INNER JOIN NOMIS.S1018252.NOMDBF95.UUGHREP

       ON     GHZXNB       = KBZXNB

         inner JOIN (SELECT GCUKCE, GCC9PC 

                              FROM (SELECT GCUKCE,

                                                GCC9PC,

                                                GCBIDT,

                                                RANK() OVER (PARTITION BY GCUKCE ORDER BY GCBIDT DESC) AS R 

                                          FROM NOMIS.S1018252.NOMDBF95.UUGCREP) A WHERE R = 1) A

       ON GHUKCE = GCUKCE

         LEFT JOIN Nomis.dbo.tblOriginators_CSR

       on SUBSTRING([KBRMCD], 1, 1)  = One_Letter_Code

                              left join(SELECT p1.CEQ9CD,

                           p1.cetutx,       

                                ( SELECT cenetx + ' ' 

                                      FROM Nomis.S1018252.NOMDBF95.CDPPHY01  p2

                                    WHERE p2.CEQ9CD = p1.CEQ9CD

                                       AND p2.cetutx = p1.cetutx

                                    ORDER BY CEQ9CD,cetutx

                                       FOR XML PATH('') ) AS PNOTE

                          FROM Nomis.S1018252.NOMDBF95.CDPPHY01 p1

                        GROUP BY CEQ9CD,p1.cetutx) as Pnote

                        on (kfq9cd = ceq9cd

                        and kftutx = cetutx)

WHERE (MCL9ND             = 2 OR MCL9ND IS NULL)

AND [KBOESU]='O' --COH OPEN CLOSED INDICATOR 

AND CASE WHEN KFQXST IS NULL OR KFQXST &lt;&gt; 'Y' 

    THEN [KFOWSU] 

    ELSE [MCVJSU] END &lt;&gt; 'C' -- COD REASON CLOSED 

    AND CASE WHEN KFQXST IS NULL OR KFQXST &lt;&gt; 'Y' 

      THEN [KFLFSU] 

    ELSE [MCVISU] END ='O' --COD LINE STATUS 

    AND GHAQNB = 1 

    AND caaqnb = 1 

    and kfaqnb = 1

    and kbaqnb = 1 

    and KFC5AA  &lt;&gt; 'FT'</property>

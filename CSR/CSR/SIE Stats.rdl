          <DataField># SO</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Selling">
          <DataField>Selling</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Qty">
          <DataField>Qty</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Cost">
          <DataField>Cost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>lcl-sql2k5-s</DataSourceName>
        <CommandText>SELECT   KFC5aa,substring(convert(char,KBAWDT),4,2) as Month,KFQXST,count(*) as [Qty of Lines]
,count(distinct kfq9cd) as [# SO],sum(kfhpva * (case when isnull(KFT4ND,0) = 0 then kfxxnb else kft4nd end)) AS [Selling],sum(case when isnull(KFT4ND,0) = 0 then kfxxnb else kft4nd end) as Qty, sum(KFI8VA * (case when isnull(KFT4ND,0) = 0 then kfxxnb else kft4nd end)) AS [Cost]
--into Nomis_warehouse.dbo.tblBoltReservedStock
FROM (NOMIS.S1018252.NOMDBF95.COHPHY01
         INNER JOIN NOMIS.S1018252.NOMDBF95.CODPHY01 ON KBAQNB = KFAQNB
              AND KBQ9CD                                       = KFQ9CD)
--		 LEFT JOIN NOMIS.S1018252.NOMDBF95.CDCPHY01
--         ON       KFAQNB = MCAQNB
--              AND KFQ9CD = MCQ9CD
--              AND MCTUTX = KFTUTX
WHERE  KBAQNB          = '1'
       --AND (MCD8NB           &lt;&gt; 0 or MCD8NB is null)
       --and CASE WHEN KFQXST is null or KFQXST &lt;&gt; 'Y' THEN KFM8TX ELSE MCXHTX END like '3051%'
       and KBAWDT &gt;= 1011001
       and kfc5aa in ('HB','HE','HF')
       --and datediff(d,CONVERT(SMALLDATETIME,(SUBSTRING(CONVERT(CHAR,KBBMNB),2,6)),12), getdate()) &gt; 5
       --and datediff(d,case when KBAWDT &lt;&gt; 0 and kbawdt &gt; 1110101 then CONVERT(SMALLDATETIME,(SUBSTRING(CONVERT(CHAR,KBAWDT),2,6)),12) else getdate() end, getdate()) &gt; 5
       --and CASE WHEN KFQXST is null or KFQXST &lt;&gt; 'Y' THEN KFEQCF ELSE MCEQCF END &lt;&gt; '9999999010'
group by KFC5aa,substring(convert(char,KBAWDT),4,2),KFQXST
ORDER BY KFC5aa</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>

        <Field Name="ID">
          <DataField>
          </DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PurchaseRequest</DataSourceName>
        <CommandText>select purchaserequestid,lclpurchaseorder,h3swtx, totalprice,sum(H3VVND * h3jjva)
from dbo.tblPurchaseRequests left join
 Nomis.S1018252.NOMDBF95.PODPHY01 
on h3swtx = lclpurchaseorder
where statusid = 2
and visible = 1
group by purchaserequestid,lclpurchaseorder,h3swtx,totalprice
order by h3swtx,lclpurchaseorder</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>

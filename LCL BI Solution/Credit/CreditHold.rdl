      <Query>
        <DataSourceName>lcl-sql2k5-s</DataSourceName>
        <CommandText>select ghzxnb, cacstx,ghupva as creditlimit, cacstx, b.*
from Nomis.NOMDBF95.UUGHREP inner join Nomis.S1018252.NOMDBF95.CUSPHY01
on ghzxnb = cazxnb
inner join Nomis.S1018252.NOMDBF95.SLSPHY01
on CARDCD = L2RDCD
left join  Nomis.dbo.ArOutStanding_Grouped b

on CustNumber= ghzxnb
where ghupva &gt; 0 and ghupva  &lt; 2
--inner join Nomis.NOMDBF95.SLSPHY01
--on CARDCD = L2RDCD
--where (cardcd = @SLSNumber or @SLSNumber = 'All')
--orDer by [Total Invoiced] + [sum of payments to date] desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>

      <Query>
        <DataSourceName>lcl-sql2k5-s</DataSourceName>
        <CommandText>select top 50 * from Nomis.dbo.ArOutStanding_Grouped  inner join Nomis.NOMDBF95.CUSPHY01 
on CustNumber= cazxnb 
inner join Nomis.NOMDBF95.SLSPHY01
on CARDCD = L2RDCD
inner join nomis.S1018252.NOMDBF95.UUGHREP
on custnumber = ghzxnb
where caaqnb = '1' and ghaqnb = '1'
orDer by ([Total Invoiced] + [sum of payments to date]) desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

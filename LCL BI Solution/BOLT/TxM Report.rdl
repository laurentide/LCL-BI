      <Query>
        <DataSourceName>lcl-sql2k5-s</DataSourceName>
        <CommandText>select * from Nomis_warehouse.dbo.tblBolt_TxM
where ([pc family] = @pcfamily or @pcfamily is null)
and ([inventory number] like '%' + @inventory + '%' or @inventory is null or [Inventory_desc_1] like '%' + @inventory + '%' )
order by [pc family], [avg cost on hand] * [qty on hand]  desc</CommandText>
        <QueryParameters>
          <QueryParameter Name="@pcfamily">
            <Value>=Parameters!pcfamily.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@inventory">
            <Value>=Parameters!inventory.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

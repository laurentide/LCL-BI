  <DataSets>
    <DataSet Name="Safety">
      <Fields>
        <Field Name="status">
          <DataField>status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="avg">
          <DataField>avg</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Safety</DataSourceName>
        <CommandText>select  status,count(*) as count,avg(datediff(d,issueddate,reviseddate)) as avg
from tblsafetycases
inner join tblsafetystatus
on tblsafetycases.statusid = tblsafetystatus.statusid
group by status</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="safetytype">
          <DataField>safetytype</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="department">
          <DataField>department</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="year">
          <DataField>year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="month">
          <DataField>month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Safety</DataSourceName>
        <CommandText>select safetytype,department,year(issueddate) as year,right('00'  + convert(varchar,month(issueddate)),2) + ' - ' + datename(month,issueddate) as month,count(*) as count
from tblsafetycases left join tblsafetytypes
on tblsafetycases.safetytypeid = tblsafetytypes.safetytypeid
left join tblsafetydepartments
on tblsafetycases.departmentid = tblsafetydepartments.departmentid
group by safetytype,department,year(issueddate),right('00'  + convert(varchar,month(issueddate)),2) + ' - ' + datename(month,issueddate)
order by safetytype,department,year(issueddate),right('00'  + convert(varchar,month(issueddate)),2) + ' - ' + datename(month,issueddate)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>

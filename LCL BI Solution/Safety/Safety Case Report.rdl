        <Field Name="status">
          <DataField>status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Safety</DataSourceName>
        <CommandText>select safetycaseid, issueddate , datediff(d,issueddate,case when reviseddate is null then getdate() else reviseddate end) , status
from dbo.tblSafetyCases a inner join tblsafetystatus b
on a.statusid = b.statusid
where a.statusid &lt;&gt;4 
and visible = 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

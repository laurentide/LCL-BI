        </Field>
        <Field Name="Misc_Text__1">
          <DataField>Misc Text #1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Print_sequence">
          <DataField>Print sequence</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Nomis_Warehouse</DataSourceName>
        <CommandText>select case when gebqcg = 'C' then GEQ9CD else GESWTX end as 'PO/SO Number',
         convert(smalldatetime,(substring(convert(char,gebwdu),2,6)),12) as [Create Date],
         case when len(convert(char,geawtn)) = 5 
          then substring(convert(char,geawtn),1,1) + ':' + substring(convert(char,geawtn),2,2)
          else substring(convert(char,geawtn),1,2) + ':' + substring(convert(char,geawtn),3,2) end  as [Create Time],
         geawvm as [Create User],
         GECYTX as [Customer PO Number],
         GEU2TY as [Misc Text #1],     
         kbqdnb as [Print sequence]
from Nomis.S1018252.NOMDBF95.PNTPHY01
left join Nomis.S1018252.NOMDBF95.COHPHY01
on kbq9cd = GEQ9CD
where  gebwdu = '1' + convert(char,@date,12)
order by gebwdu desc,geawtn desc</CommandText>
        <QueryParameters>
          <QueryParameter Name="@date">
            <Value>=Parameters!date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
